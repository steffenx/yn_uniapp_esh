<view data-event-opts="{{[['touchstart',[['refreshStart',['$event']]]],['touchmove',[['refreshMove',['$event']]]],['touchend',[['refreshEnd',['$event']]]],['tap',[['conClick',['$event']]]]]}}" class="content" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e" bindtap="__e"><refresh class="vue-ref" bind:isRefresh="__e" vue-id="1" data-ref="refresh" data-event-opts="{{[['^isRefresh',[['isRefresh']]]]}}" bind:__l="__l"></refresh><click-circle class="vue-ref" vue-id="2" data-ref="clickCircle" bind:__l="__l"></click-circle><view class="l-body"><view class="header"><swiper class="l-swiper" indicator-dots="{{true}}" autoplay="{{true}}" interval="{{3000}}" duration="{{500}}" indicator-color="grey" indicator-active-color="#41bdb2" circular><block wx:for="{{images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="l-swiper-item"><image class="l-carousel-img" src="{{item.img}}" mode="scaleToFill"></image></view></swiper-item></block></swiper></view><view class="l-body-tab"><block wx:for="{{navTab}}" wx:for-item="d" wx:for-index="n" wx:key="n"><view class="navTab"><view data-event-opts="{{[['tap',[['nav_guide',['$0'],[[['navTab','',n,'id']]]]]]]}}" class="{{['navTabimg',d.background]}}" bindtap="__e"><image class="img_guide" src="{{d.img_guide}}" mode="aspectFit"></image></view><view class="navTabItem">{{d.tab_value}}</view></view></block></view><view class="swiper_box"><view class="circle_box"><view class="circle_top">{{score}}</view><view class="circle_bottom">总积分</view></view><view class="text_box"><view>{{"投递次数："+num}}</view><view>{{"上次积分："+fen}}</view></view></view><block wx:for="{{distribution}}" wx:for-item="m" wx:for-index="i" wx:key="i"><view data-event-opts="{{[['tap',[['map_distance',['$0'],[[['distribution','',i,'id']]]]]]]}}" class="distribution" bindtap="__e"><view class="distance_name"><view class="local_left"><image class="img_position" src="../../static/images/icon_location_point.png" mode="aspectFit"></image><text class="text_position">{{m.position}}</text></view><view class="local_right"><text class="distance">{{"约"+m.distance}}</text><uni-icon vue-id="{{'3-'+i}}" type="map" size="20" bind:__l="__l"></uni-icon></view></view><view class="distance_icon"><block wx:for="{{icons}}" wx:for-item="d" wx:for-index="txt" wx:key="txt"><view class="icon_bottom"><block wx:if="{{d.state==1}}"><image class="icon_state"></image></block><block wx:if="{{d.state==2}}"><image class="icon_state" src="../../static/images/tag_2_wh.png"></image></block><block wx:if="{{d.state==3}}"><image class="icon_state" src="../../static/images/tag_2_zm.png"></image></block><image class="icon_img" src="{{d.icon_img}}" mode="scaleToFill"></image><view>{{''+d.icon_name+''}}</view></view></block></view></view></block></view></view>